@using EntityLayer.Concrete
@model List<Category>

@{
    Layout = null;
}

<table>
    <tr>
        <td>ID</td>
        <td>Kategori Adı</td>
        <td>Durumu</td>
        <td>Detaylar</td>
        <td>Sil</td>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.CategoryID</td>
            <td>@item.CategoryName</td>
            <td>@item.CategoryStatus</td>
            <td><a href="#" class="btn btn-warning">Detaylar</a></td>
        </tr>

    }
</table>